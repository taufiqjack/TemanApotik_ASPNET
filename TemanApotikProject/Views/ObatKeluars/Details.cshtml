@model TemanApotikProject.Models.ObatKeluar

@{
    ViewBag.Title = "Detail";
}

<h2>Detail Obat Keluar</h2>

<div>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayName("Tanggal Keluar")
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Tgl_Keluar)
        </dd>

        <dt>
            @Html.DisplayName("Nama Pasien")
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Pasien.Nama_Pasien)
        </dd>

        <dt>
            @Html.DisplayName("Nama Obat")
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Obat.id_Obat)
        </dd>

        <dt>
            @Html.DisplayName("Jenis Obat")
        </dt>

        <dd>
            @Html.DisplayFor(model => model.id_Jenis_Obat)
        </dd>

        <dt>
            @Html.DisplayName("Jumlah Keluar")
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Jumlah_Keluar)
        </dd>

        <dt>
            @Html.DisplayName("Total Harga")
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Total_Harga)
        </dd>
    </dl>
</div>
<p>
    @Html.ActionLink("Edit", "Edit", new { id = Model.id_Transaksi_Keluar }) |
    @Html.ActionLink("Kembali ke List", "Index")
</p>
